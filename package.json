{
  "name": "global-store-repository",
  "private": true,
  "homepage": "https://github.com/unional/global-store#readme",
  "bugs": {
    "url": "https://github.com/unional/global-store/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unional/global-store.git"
  },
  "license": "MIT",
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean",
    "coverage": "turbo run coverage",
    "cs": "changeset",
    "depcheck": "turbo run depcheck",
    "global-store": "pnpm --filter=global-store",
    "lint": "turbo run lint",
    "nuke": "turbo run nuke",
    "nuke:local": "rimraf node_modules || true",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "prepare": "husky install",
    "publish": "changeset publish",
    "size": "turbo run size",
    "test": "turbo run test",
    "verify": "turbo run build lint coverage depcheck size",
    "version": "changeset version && pnpm install"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/plugin-syntax-flow": "^7.18.6",
    "@babel/plugin-transform-react-jsx": "^7.18.10",
    "@changesets/cli": "^2.26.1",
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-harmony": "^7.1.2",
    "husky": "^8.0.3",
    "pinst": "^3.0.0",
    "turbo": "^1.9.3",
    "typescript": "^5.0.4"
  },
  "packageManager": "pnpm@8.3.1"
}
