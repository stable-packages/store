{
  "name": "global-store",
  "description": "A version locked global store.",
  "version": "0.8.1",
  "main": "dist-es5/index.js",
  "module": "dist-es2015/index.js",
  "typings": "dist-es5/index.d.ts",
  "files": [
    "dist",
    "dist-es5",
    "dist-es2015",
    "flow-typed",
    "index.js.flow"
  ],
  "scripts": {
    "build": "npm run build-es5 && npm run build-es2015 && npm run bundle",
    "build-es5": "tsc -p tsconfig.es5.json",
    "build-es2015": "tsc -p tsconfig.es2015.json",
    "bundle": "webpack",
    "clean": "rimraf dist dist-es5 dist-es2015",
    "coverage": "jest --coverage",
    "dependency-check": "dependency-check . --unused --no-dev && dependency-check . --missing --no-dev",
    "lint": "tslint -p tsconfig.json 'src/**/*.ts'",
    "postpublish": "git push",
    "preversion": "git pull && npm run clean && npm run verify && npm run bundle",
    "start": "jest --watch",
    "test": "tsc && jest",
    "verify": "npm run lint && npm run build && npm run coverage && npm run dependency-check",
    "verify-ci": "npm run lint && npm run build && jest --coverage --config jest.config.ci.json && npm run dependency-check",
    "watch": "jest --watch"
  },
  "author": {
    "name": "Homa Wong",
    "email": "homawong@gmail.com"
  },
  "homepage": "https://github.com/unional/global-store",
  "bugs": {
    "url": "https://github.com/unional/global-store/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/unional/global-store.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^24.0.0",
    "@types/node": "^11.9.0",
    "assertron": "^6.1.0",
    "aurelia-logging": "^1.5.1",
    "aurelia-polyfills": "^1.3.3",
    "babel-eslint": "^10.0.1",
    "dependency-check": "^3.3.0",
    "eslint": "^5.13.0",
    "eslint-plugin-flowtype": "^3.2.1",
    "eslint-plugin-harmony": "^1.0.4",
    "greenkeeper-lockfile": "^1.15.1",
    "jest": "^24.1.0",
    "jest-audio-reporter": "^2.2.1",
    "jest-watch-suspend": "^1.1.1",
    "jest-watch-toggle-config": "^1.0.2",
    "param-case": "^2.1.1",
    "pascal-case": "^2.0.1",
    "rimraf": "^2.6.3",
    "rollup": "^0.67.4",
    "rollup-plugin-typescript": "^1.0.0",
    "source-map-loader": "^0.2.4",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.3",
    "tslib": "^1.9.3",
    "tslint": "^5.12.1",
    "tslint-config-unional": "^0.10.0",
    "tslint-language-service": "^0.9.9",
    "type-plus": "^1.12.1",
    "typescript": "^3.3.3",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3"
  },
  "dependencies": {}
}
