{
  "name": "global-store",
  "private": true,
  "homepage": "https://github.com/unional/global-store#readme",
  "bugs": {
    "url": "https://github.com/unional/global-store/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unional/global-store.git"
  },
  "license": "MIT",
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean",
    "coverage": "turbo run coverage",
    "cs": "changeset",
    "depcheck": "turbo run depcheck",
    "global-store": "pnpm --filter=global-store",
    "lint": "turbo run lint",
    "nuke": "turbo run nuke",
    "nuke:local": "rimraf node_modules || true",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "prepare": "husky install",
    "publish": "changeset publish",
    "size": "turbo run size",
    "test": "turbo run test",
    "verify": "turbo run build lint coverage depcheck size",
    "version": "changeset version && pnpm install"
  },
  "devDependencies": {
    "@changesets/cli": "^2.22.0",
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "husky": "^8.0.1",
    "pinst": "^3.0.0",
    "turbo": "^1.4.3",
    "typescript": "^4.7.4"
  },
  "packageManager": "pnpm@7.9.1"
}
